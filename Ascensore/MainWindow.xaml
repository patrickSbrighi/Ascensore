        <Button x:Name="piano3" Content="3" HorizontalAlignment="Left" Height="83" Margin="180,231,0,0" VerticalAlignment="Top" Width="54"/>
        <Button x:Name="piano4" Content="4" HorizontalAlignment="Left" Height="83" Margin="89,319,0,0" VerticalAlignment="Top" Width="54"/>
        <Button x:Name="piano5" Content="5" HorizontalAlignment="Left" Height="88" Margin="161,319,0,0" VerticalAlignment="Top" Width="54"/>
        <Image x:Name="imgAscensore" Margin="420,246,290,277" Source="DisegnoAscensore.png" Stretch="Fill"/>

    </Grid>
</Window>
